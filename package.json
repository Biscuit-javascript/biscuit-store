{
  "private": true,
  "description": "Library for managing javascript application states",
  "author": "Zhulev Philipp",
  "version": "0.8.5",
  "workspaces": [
    "packages/*",
    "expemples/*"
  ],
  "keywords": [
    "javascript",
    "react",
    "state",
    "library",
    "state-managment",
    "es6"
  ],
  "repository": "https://github.com/Biscuit-javascript/biscuit-store.git",
  "bugs": {
    "url": "https://github.com/Biscuit-javascript/biscuit-store/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "parcel ./exemples/index.html --open",
    "build": "parcel build ./exemples/index.html",
    "test": "lerna run --parallel test",
    "prettier": "prettier ./packages/*/**/*.{js,ts}",
    "lint": "eslint ./packages/*/**/*.{js,ts}",
    "release:publish": "lerna publish from-package --dist-tag next",
    "cangelog:add": "changeset add",
    "cangelog:ver": "changeset verison",
    "cangelog:publish": "changeset publish"
  },
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run test"
    }
  },
  "lint-staged": {
    "./packages/*/{src,tests}/*.(js|ts)": [
      "yarn run lint"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "@changesets/cli": "^2.1.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-preset-react-app": "^9.1.2",
    "camelcase": "^5.3.1",
    "eslint": "^6.6.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "husky": "^4.3.8",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "parcel-bundler": "^1.6.1",
    "prettier": "^2.2.1",
    "sonarjs": "^1.0.0",
    "typescript": "3.8.3"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "publishConfig": {
    "access": "public"
  }
}
